{
  "name": "md-to-slidev",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "server"
  ],
  "scripts": {
    "dev:deck": "cd apps/deck && npx slidev",
    "dev:web": "cd apps/web && npm run dev",
    "dev:api": "cd server && npx ts-node index.ts",
    "dev": "npm-run-all --parallel dev:deck dev:web dev:api"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
